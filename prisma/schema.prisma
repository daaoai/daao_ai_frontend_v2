datasource db {
  provider = "mysql"
  url      = "mysql://root:a@localhost:3306/WaitingList" // e.g. mysql://root:password@localhost:3306/mydb
}

generator client {
  provider = "prisma-client-js"
}

model WaitingList {
  id        Int      @id @default(autoincrement())
  email     String
  createdAt DateTime @default(now())
}